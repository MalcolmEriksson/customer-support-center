{
    "$schema": "https://json.schemastore.org/nest-cli",
    "collection": "@nestjs/schematics",
    "sourceRoot": "src",
    "compilerOptions": {
        "deleteOutDir": true,
        "webpack": true
    },
    "projects": {
        "agent-db": {
            "type": "library",
            "root": "libs/agent-db",
            "entryFile": "index",
            "sourceRoot": "libs/agent-db/src",
            "compilerOptions": {
                "tsConfigPath": "libs/agent-db/tsconfig.lib.json"
            }
        },
        "case-db": {
            "type": "library",
            "root": "libs/case-db",
            "entryFile": "index",
            "sourceRoot": "libs/case-db/src",
            "compilerOptions": {
                "tsConfigPath": "libs/case-db/tsconfig.lib.json"
            }
        }
    }
}
